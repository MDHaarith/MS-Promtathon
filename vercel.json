{
  "buildCommand": "pnpm build",
  "outputDirectory": "dist/spa",
  "devCommand": "pnpm dev",
  "framework": "vite",
  "nodeVersion": "22.x",
  "env": {
    "NODE_ENV": "production"
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "dist/server/production.mjs"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
